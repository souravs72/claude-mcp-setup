# Example secrets file for Helm deployment
# Copy this to secrets.yaml and fill in your actual values
# WARNING: Never commit secrets.yaml to version control!

secrets:
  # GitHub Personal Access Token (required for github-server)
  githubToken: "ghp_YOUR_GITHUB_TOKEN_HERE"
  
  # Jira API Token (required for jira-server)
  jiraApiToken: "YOUR_JIRA_API_TOKEN_HERE"
  
  # Google Cloud API Key (required for internet-server)
  googleApiKey: "YOUR_GOOGLE_API_KEY_HERE"
  
  # Google Custom Search Engine ID (required for internet-server)
  googleSearchEngineId: "YOUR_GOOGLE_SEARCH_ENGINE_ID_HERE"
  
  # Frappe credentials (optional, if using frappe-server)
  frappeApiKey: "YOUR_FRAPPE_API_KEY"
  frappeApiSecret: "YOUR_FRAPPE_API_SECRET"
  
  # PostgreSQL password (auto-generated if using Terraform)
  postgresPassword: "CHANGE_THIS_TO_SECURE_PASSWORD"

# Update image registry if using custom registry
global:
  imageRegistry: "YOUR_REGISTRY/mcp-servers"
  imageTag: "latest"
  
# Override specific configurations
servers:
  jira:
    env:
      JIRA_BASE_URL: "https://your-company.atlassian.net"
      JIRA_EMAIL: "your-email@company.com"
      JIRA_PROJECT_KEY: "YOUR_PROJECT"

